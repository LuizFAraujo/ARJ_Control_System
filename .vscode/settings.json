
// Files: Exclude
{
    "files.exclude": {
        "public": true,
        "_ OTHER": true,
        "_": true,
        "@types": true,
        ".vscode": true,
        ".next": true,
        "node_modules": true,

        ".gitignore": true,

        "LICENSE": true,
        
